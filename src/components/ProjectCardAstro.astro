---
import { Image } from "astro:assets";
import type { Title, CoverPicture } from "../utils/ApiCall";
const {  title, coverPicture,id } = Astro.props;
---

<a
class="h-96 p-6  overflow-hidden  bg-white rounded-lg border border-gray-200 shadow-md dark:bg-gray-800 dark:border-gray-700"

href={`/${title}`}>
<Image alt={title} style={`view-transition-name:project-${id}`} src={coverPicture.data.attributes.url} format="avif" width={100} height={100} class="object-cover w-full h-full "/>
</a>